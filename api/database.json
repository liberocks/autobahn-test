{
    "sql-file": true,
    "defaultEnv": "local",
    "local": {
        "driver": "mysql",
        "port": { "ENV": "DB_PORT" },
        "host": { "ENV": "DB_HOST" },
        "user": { "ENV": "DB_USER" },
        "password": { "ENV": "DB_PASSWORD" },
        "database": { "ENV": "DB_NAME" },
        "multipleStatements": true
    }
}